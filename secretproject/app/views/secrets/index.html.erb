<table>
  <tr>
    <th> Secrets </th>
  </tr>
  <%@secret.each do |x|%>
    <tr>
      <td><%=x.content%>
        <form action='/secrets/delete/<%=x.id%>' method='post'>
          <input type="hidden" name="_method" value="delete">
          <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
          <button type="submit" >Delete Secret</button>
        </form></td>
    </tr>
  <%end%>
</table>
